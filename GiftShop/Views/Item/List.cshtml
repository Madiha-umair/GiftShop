

@model IEnumerable<GiftShop.Models.ItemDto>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<a href="/Item/New">Add New Item</a>
@foreach (var item in Model)
{
    <h2><a href="/Item/Details/@item.ItemId"> Item Name: @item.ItemName</a></h2>
    <h2>Item Description: @item.ItemDescription</h2>
  
}